{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "cli.fido2.attest",
  "type": "object",
  "required": ["event", "status", "data"],
  "properties": {
    "event": { "type": "string", "enum": ["cli.fido2.attest"] },
    "status": { "type": "string" },
    "reasonCode": { "type": "string" },
    "telemetryId": { "type": "string" },
    "sanitized": { "type": "boolean" },
    "data": {
      "type": "object",
      "required": ["attestation"],
      "properties": {
        "attestation": {
          "type": "object",
          "required": ["id", "rawId", "type", "format", "response"],
          "properties": {
            "id": { "type": "string" },
            "rawId": { "type": "string" },
            "type": { "type": "string" },
            "format": { "type": "string" },
            "response": {
              "type": "object",
              "required": ["clientDataJSON", "attestationObject"],
              "properties": {
                "clientDataJSON": { "type": "string" },
                "attestationObject": { "type": "string" }
              },
              "additionalProperties": true
            }
          },
          "additionalProperties": true
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true,
  "examples": [
    {
      "event": "cli.fido2.attest",
      "status": "success",
      "reasonCode": "generated",
      "telemetryId": "cli-fido2-telemetry-id",
      "sanitized": true,
      "data": {
        "attestation": {
          "id": "BASE64URL_ID",
          "rawId": "BASE64URL_ID",
          "type": "public-key",
          "format": "packed",
          "response": {
            "clientDataJSON": "BASE64URL",
            "attestationObject": "BASE64URL"
          }
        }
      }
    }
  ]
}
